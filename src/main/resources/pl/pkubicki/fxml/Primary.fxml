<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="CENTER" hgap="20" styleClass="root" vgap="20" xmlns="http://javafx.com/javafx/8.0.271" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.pkubicki.controllers.PrimaryController">
   <padding><Insets bottom="25" left="25" right="25" top="25" /></padding>
   <gridLinesVisible>true</gridLinesVisible>
   <Text id="welcome-text" text="Path Player 1.0 Welcome" GridPane.columnIndex="0" GridPane.columnSpan="2" GridPane.rowIndex="0" />
<HBox GridPane.columnIndex="0" GridPane.rowIndex="1">
   <Button fx:id="secondaryControllerButton" onAction="#switchToSecondary" text="Secondary Con" />
</HBox>
   <HBox GridPane.columnIndex="1" GridPane.rowIndex="3">
      <Button fx:id="routeTravelButton" onAction="#switchToRouteTravel" text="Routing Travel" />
   </HBox>
   <HBox GridPane.columnIndex="0" GridPane.rowIndex="2">
      <Button fx:id="addPoiButton" onAction="#openUploadForm" text="Add POI" />
   </HBox>
   <HBox GridPane.columnIndex="1" GridPane.rowIndex="2">
      <Button fx:id="downloadPoiButton" onAction="#openDownloadForm" text="Download POI" />
   </HBox>
   <HBox GridPane.columnIndex="0" GridPane.rowIndex="3">
      <Button fx:id="freeTravelButton" onAction="#openFreeTravelWindow" text="Free Travel" />
   </HBox>
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
   <stylesheets>
      <URL value="@../css/FreeTravel.css" />
   </stylesheets>
</GridPane>
