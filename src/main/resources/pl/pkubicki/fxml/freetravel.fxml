<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="center" hgap="10" styleClass="root" vgap="10" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.pkubicki.controllers.FreeTravelController">
    <padding><Insets bottom="10" left="25" right="25" top="25" /></padding>
    <gridLinesVisible>true</gridLinesVisible>
    <Label text="Search:" GridPane.columnIndex="0" GridPane.rowIndex="0" />

    <TextField fx:id="searchText" prefWidth="400.0" GridPane.columnIndex="1" GridPane.rowIndex="0" />
    <Label text="Search results:" GridPane.rowIndex="1" />
    <ChoiceBox fx:id="searchResultsChoiceBox" prefHeight="25.0" prefWidth="400.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />

    <Label text="Latitude:" GridPane.columnIndex="0" GridPane.rowIndex="2" />

    <TextField fx:id="latitudeText" prefWidth="380.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />

    <Label text="Longitude:" GridPane.columnIndex="0" GridPane.rowIndex="3" />

    <TextField fx:id="longitudeText" prefWidth="380.0" GridPane.columnIndex="1" GridPane.rowIndex="3" />

    <Label text="Proximity[m]:" GridPane.columnIndex="0" GridPane.rowIndex="4" />

    <TextField fx:id="proximityText" prefWidth="380.0" GridPane.columnIndex="1" GridPane.rowIndex="4" />

    <ChoiceBox fx:id="unitChoiceBox" GridPane.columnIndex="2" GridPane.rowIndex="4" />

    <TextField fx:id="startPoiText" editable="false" prefWidth="380.0" GridPane.columnIndex="1" GridPane.rowIndex="5" />

<!--    <TextArea fx:id="proximityListText" GridPane.columnIndex="2" GridPane.rowIndex="8" />-->

     <Button fx:id="createPoiListButton" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" onAction="#createProximityListWithLabels" prefHeight="25.0" prefWidth="373.0" text="Create POI list in vicinity" GridPane.columnIndex="1" GridPane.rowIndex="6" />
     <Button fx:id="searchButton" onAction="#createGeoLocation" text="Search" GridPane.columnIndex="2" />
   <Label text="Start Point:" GridPane.rowIndex="5" />
   <Button fx:id="checkButton" mnemonicParsing="false" onAction="#makeStartPoiFromGps" text="Check" GridPane.columnIndex="2" GridPane.rowIndex="5" />
   <GridPane GridPane.columnIndex="1" GridPane.rowIndex="8">
     <columnConstraints>
       <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
